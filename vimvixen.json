{
  "keymaps": {
    "k": { "type": "scroll.vertically", "count": -1 },
    "<C-e>": { "type": "scroll.vertically", "count": -1 },
    "j": { "type": "scroll.vertically", "count": 1 },
    "<C-y>": { "type": "scroll.vertically", "count": 1 },
    "gg": { "type": "scroll.top" },
    "G": { "type": "scroll.bottom" },
    "u": { "type": "scroll.pages", "count": -0.5 },
    "d": { "type": "scroll.pages", "count": 0.5 },
    "h": { "type": "scroll.horizonally", "count": -1 },
    "l": { "type": "scroll.horizonally", "count": 1 },
    "r": { "type": "tabs.reload", "cache": false },
    "yy": { "type": "urls.yank" },
    "p": { "type": "urls.paste", "newTab": false },
    "P": { "type": "urls.paste", "newTab": true },
    "f": { "type": "follow.start", "newTab": false },
    "F": { "type": "follow.start", "newTab": true, "background": false },
    "o": { "type": "command.show.open", "alter": false },
    "O": { "type": "command.show.tabopen", "alter": false },
    "T": { "type": "command.show.buffer" },
    "ge": { "type": "command.show.open", "alter": true },
    "gE": { "type": "command.show.tabopen", "alter": true },
    "/": { "type": "find.start" },
    "n": { "type": "find.next" },
    "N": { "type": "find.prev" },
    "H": { "type": "navigate.history.prev" },
    "L": { "type": "navigate.history.next" },
    "J": { "type": "tabs.prev" },
    "K": { "type": "tabs.next" },
    "g0": { "type": "tabs.first" },
    "g$": { "type": "tabs.last" },
    "yt": { "type": "tabs.duplicate" },
    "<A-p>": { "type": "tabs.pin.toggle" },
    "x": { "type": "tabs.close" },
    "X": { "type": "tabs.reopen" },
    "W": { "type": "command.show.winopen", "alter": true },
    "gf": { "type": "page.source" },
    "zH": { "type": "scroll.home" },
    "zL": { "type": "scroll.end" },
    "m": { "type": "mark.set.prefix" },
    "gu": { "type": "navigate.parent" },
    "gU": { "type": "navigate.root" }
  },
  "search": {
    "default": "searx.be",
    "engines": {
      "searx.be": "https://searx.be/?q={}",
      "yt": "https://yewtu.be/search?q={}",
      "arch": "https://wiki.archlinux.org/index.php?search={}"
    }
  },
  "properties": {
	      "hintchars": "abcdefghijklmnopqrstuvwxyz",
	      "smoothscroll": false,
	      "complete": "sbh",
	      "colorscheme": "dark"
  },
  "blacklist": [
	    ]
}
